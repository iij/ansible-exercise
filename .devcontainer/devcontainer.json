{
    "name": "ansible-exercise",
    "dockerComposeFile": "../docker-compose/docker-compose.yml",
    "service": "ansible",
    "workspaceFolder": "/ansible-exercise",
    "shutdownAction": "stopCompose",
    "remoteEnv": {
        "LANG": "en_US.UTF-8"
    },
    "extensions": [
        "wholroyd.Jinja",
        "vscoss.vscode-ansible"
    ],
    "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
    }
}